<Files .htaccess>
    Order allow,deny
    Deny from all
</Files>

ErrorDocument 404 /Pages/404.php
ErrorDocument 403 /Pages/403.php
ErrorDocument 401 /Pages/401.php

AuthUserFile /OSPanel/domains/TestingSystem/.htpasswd
AuthUserFile /TestingSystemBuild/domains/TestingSystem/.htpasswd
AuthType Basic
AuthName "Access to Panel"

<FilesMatch "panel.php">
  Require valid-user
</FilesMatch>

Options -Indexes

IndexIgnore *

RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^(.*)$ $1.php [NC,L]
#RewriteRule ^program(/|/.+)?$ - [R=404,L]